{
  "name": "idb-polyfill",
  "version": "0.1.0",
  "description": "IndexedDB polyfill",
  "repository": "treojs/idb-polyfill",
  "license": "Apache-2.0",
  "main": "lib/idb-polyfill.js",
  "files": [
    "lib"
  ],
  "keywords": [
    "indexeddb",
    "polyfill",
    "in-memory",
    "idb",
    "shim"
  ],
  "scripts": {
    "prepublish": "babel src --out-dir lib --presets es2015",
    "build": "browserify -t [ babelify --presets es2015 ] src/idb-polyfill.js -o dist/idb-polyfill.js -s idbPolyfill",
    "qunit": "node-qunit-phantomjs ./test/indexedDBmock/index.html",
    "test": "eslint src/ && mocha test/w3c test/fakeIndexedDB && npm run qunit"
  },
  "dependencies": {
    "array.prototype.find": "^1.0.0",
    "array.prototype.findindex": "^1.0.0",
    "deets": "^1.0.1",
    "inherits": "^2.0.1",
    "is-integer": "^1.0.6",
    "is-plain-obj": "^1.1.0",
    "object-values": "^1.0.0",
    "polyfill-function-prototype-bind": "0.0.1",
    "setimmediate": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.20",
    "babel-eslint": "^4.1.5",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "node-qunit-phantomjs": "^1.3.0",
    "phantomjs-polyfill": "0.0.1",
    "qunitjs": "^1.20.0"
  }
}
